<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GameOfLifeController">
   <children>
      <ImageView fitHeight="463.0" fitWidth="608.0" layoutX="-2.0">
         <image>
            <Image url="@BACKGROUND.png" />
         </image>
      </ImageView>
      <Button layoutX="60.0" layoutY="339.0" mnemonicParsing="false" onAction="#twoPlayerSystem" prefHeight="56.0" prefWidth="155.0" text="2 Players" textAlignment="CENTER">
         <font>
            <Font name="Century Gothic" size="18.0" />
         </font>
      </Button>
      <Button layoutX="234.0" layoutY="339.0" mnemonicParsing="false" onAction="#threePlayerSystem" prefHeight="56.0" prefWidth="155.0" text="3 Players" textAlignment="CENTER">
         <font>
            <Font name="Century Gothic" size="18.0" />
         </font>
      </Button>
      <Button layoutX="411.0" layoutY="339.0" mnemonicParsing="false" onAction="#exitApp" prefHeight="56.0" prefWidth="155.0" text="Exit" textAlignment="CENTER">
         <font>
            <Font name="Century Gothic" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
